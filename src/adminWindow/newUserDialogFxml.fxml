<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminWindow.NewUserDialogController">

    <headerText>
        Enter details
    </headerText>

    <content>
        <GridPane hgap="10" vgap="10">
            <padding>
                <Insets bottom="10" left="10" right="10" top="10" />
            </padding>

            <Label text="Email" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <TextField fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="0" />

            <Label text="Password" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <TextField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1" />

            <Label text="First name" GridPane.columnIndex="0" GridPane.rowIndex="2" />
            <TextField fx:id="firstName" GridPane.columnIndex="1" GridPane.rowIndex="2" />

            <Label text="Last name" GridPane.columnIndex="0" GridPane.rowIndex="3" />
            <TextField fx:id="lastName" GridPane.columnIndex="1" GridPane.rowIndex="3" />

         <Label fx:id="warningLabel" prefHeight="17.0" prefWidth="148.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
        </GridPane>
    </content>

</DialogPane>
